<!DOCTYPE configuration PUBLIC "-//JHelioviewer//DTD Channel Configuration//EN"
  "conf.dtd">
<configuration>
  <global>
    <stream-server>150.214.150.68</stream-server>
    <stream-port>4551</stream-port>
    <stream-pass>changeme</stream-pass>
    <stream-type>video/ogg</stream-type>

    <jnlp-path>jhelioviewer.jnlp</jnlp-path>

    <img-base>/home/gabor/img/delphi.nascom.nasa.gov/jp2</img-base>
  </global>

  <entry xml:id="full2048" mode="loop-dir">
    <mount-point>full2048.ogg</mount-point>
    <name>Full 2048x2048 - May 26</name>
    <desc>Full image of the Sun in 2048x2048 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>2048</resolution>
    <sec-per-img>3</sec-per-img>
  </entry>

  <entry xml:id="full1024" mode="loop-dir">
    <mount-point>full1024.ogg</mount-point>
    <name>Full 1024x1024 - May 26</name>
    <desc>Full image of the Sun in 1024x1024 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>1024</resolution>
    <sec-per-img>3</sec-per-img>
  </entry>
  
  <entry xml:id="full512" mode="loop-dir">
    <mount-point>full512.ogg</mount-point>
    <name>Full 512x512 - May 26</name>
    <desc>Full image of the Sun in 512x512 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>512</resolution>
    <sec-per-img>3</sec-per-img>
    <dateformat>+%Y-%m-%d %H:%M:%S</dateformat>
    <subchannels>
      <channel href="topleft512"/>
      <channel href="topright512"/>
      <channel href="bottomleft512"/>
      <channel href="bottomright512"/>
    </subchannels>
  </entry>

  <entry xml:id="topleft512" mode="loop-dir">
    <mount-point>topleft512.ogg</mount-point>
    <name>Top-left 512x512 - May 26</name>
    <desc>Top-left view of the Sun in 512x512 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>1024</resolution>
    <sec-per-img>3</sec-per-img>
    <dateformat>+%Y-%m-%d %H:%M:%S</dateformat>
    <region>
      <top>0.0</top>
      <left>0.0</left>
      <height>0.5</height>
      <width>0.5</width>
    </region>
  </entry>

  <entry xml:id="topright512" mode="loop-dir">
    <mount-point>topright512.ogg</mount-point>
    <name>Top-right 512x512 - May 26</name>
    <desc>Top-right view of the Sun in 512x512 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>1024</resolution>
    <sec-per-img>3</sec-per-img>
    <dateformat>+%Y-%m-%d %H:%M:%S</dateformat>
    <region>
      <top>0.0</top>
      <left>0.5</left>
      <height>0.5</height>
      <width>0.5</width>
    </region>
  </entry>

  <entry xml:id="bottomleft512" mode="loop-dir">
    <mount-point>bottomleft512.ogg</mount-point>
    <name>Bottom-left 512x512 - May 26</name>
    <desc>Bottom-left view of the Sun in 512x512 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>1024</resolution>
    <sec-per-img>3</sec-per-img>
    <region>
      <top>0.5</top>
      <left>0.0</left>
      <height>0.5</height>
      <width>0.5</width>
    </region>
  </entry>

  <entry xml:id="bottomright512" mode="loop-dir">
    <mount-point>bottomright512.ogg</mount-point>
    <name>Bottom-right 512x512 - May 26</name>
    <desc>Bottom-right view of the Sun in 512x512 resolution from 2012 May 26</desc>
    <source>AIA/1600/2012/05/26</source>
    <fps>4</fps>
    <resolution>1024</resolution>
    <sec-per-img>3</sec-per-img>
    <dateformat>+%Y-%m-%d %H:%M:%S</dateformat>
    <region>
      <top>0.5</top>
      <left>0.5</left>
      <height>0.5</height>
      <width>0.5</width>
    </region>
  </entry>
</configuration>
